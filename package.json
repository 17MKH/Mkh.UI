{
  "id": 0,
  "name": "mkh-ui",
  "label": "",
  "version": "1.4.0-beta.005",
  "icon": "document",
  "description": "17MKH前端框架",
  "author": "OLDLI",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "type": "module",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "vite --host --config=./build/app.config.ts",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build --config=./build/app.config.ts",
    "lib": "npm run clean && vite build --config=./build/lib.config.ts && npm run locales && npm run plugins && npm run build:dts",
    "build:dts": "npm run build-temp-types && npm run patch-types && api-extractor run && npm run build:plguins:dts && rimraf ./.temp",
    "build:plguins:dts": "api-extractor run -c api-extractor.plugins.module.json && api-extractor run -c api-extractor.plugins.skin.json",
    "build-temp-types": "tsc --emitDeclarationOnly -p ./tsconfig.lib.json --outDir ./.temp",
    "patch-types": "ts-node-esm ./build/patch-types.ts",
    "locales": "rollup -c ./build/locales.config.ts --configPlugin typescript",
    "plugins": "rollup -c ./build/plugin.config.ts --configPlugin typescript",
    "clean": "rimraf lib && rimraf public && rimraf dist && rimraf ./.temp",
    "cm": "rimraf package-lock.json && rimraf node_modules",
    "cv": "rimraf node_modules/.vite"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "clipboard-copy": "^4.0.1",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.1",
    "highlight.js": "^11.7.0",
    "lodash": "^4.17.21",
    "mkh-ui-style": "^1.0.7",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.0",
    "tslib": "^2.5.0",
    "vue": "^3.2.47",
    "vue-echarts": "^6.5.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/echarts": "^4.9.16",
    "@types/ejs": "^3.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/node": "^18.15.11",
    "@types/nprogress": "^0.2.0",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "ejs": "^3.1.9",
    "eslint": "^8.37.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "html-minifier-terser": "^7.1.0",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-standard": "^31.0.0",
    "terser": "^5.16.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  },
  "files": [
    "entries",
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
